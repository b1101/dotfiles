alias -g pacman="sudo pacman"
alias       pms="pacman -Sy"
alias       pmy="pacman -Syyu"
alias       pmq="pacman -Ss"
alias       pmr="pacman -R"
alias       pmm="makepkg -sric"

alias -g pacman-key="sudo pacman-key"
alias       pkr="sudo pacman-key -r"
alias       pks="sudo pacman-key --lsign"
# vim: ft=zsh
