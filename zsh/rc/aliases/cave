alias -g cave='sudo cave'
alias    cy='cave sync && cave generate-metadata && slow cave resolve world -c -Cs'
alias    cx='slow cave resolve world -cx -Cs'
alias    cY='cave sync && cave generate-metadata && slow cave resolve world -cx -Cs'
alias -g cr='slow cave resolve -Cs'
alias -g cR='cave resolve -x -Cs'
alias -g cu='cave uninstall'
alias -g cU='cave uninstall -x'
alias    cP='cave purge'
alias -g ci='cave import'
alias -g cI='cave import -x'
alias -g cs='cave search'
alias -g cS='cave show'
alias -g cC='cave contents'
alias -g cw='cave update-world'
alias -g cW='cave update-world -r'
# vim: ft=zsh
